<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN" "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">
<generatorConfiguration>
	<context id="context1">
		<commentGenerator>
			<!-- 是否去除自动生成的注释 true：是 ： false:否 -->
			<property name="suppressAllComments" value="true" />
		</commentGenerator>
		<!-- 数据库链接URL、用户名、密码 -->
		<jdbcConnection driverClass="com.mysql.jdbc.Driver"
			connectionURL="jdbc:mysql://192.168.56.101:3306/testbridge"
			userId="root" password="123456" />
		<!-- 生成模型的包名和位置 -->
		<javaModelGenerator
			targetPackage="com.educoder.bridge.sys.model"
			targetProject="src/main/java">
			<property name="enableSubPackages" value="true" />
		</javaModelGenerator>
		<!-- 生成的映射文件报名和位置 -->
		<sqlMapGenerator targetPackage="mapper.sys"
			targetProject="src/main/resources">
			<property name="enableSubPackages" value="true" />
		</sqlMapGenerator>
		<!-- 生成DAO的包名和位置 -->
		<javaClientGenerator
			targetPackage="com.educoder.bridge.sys.dao"
			targetProject="src/main/java" type="XMLMAPPER">
			<property name="enableSubPackages" value="true" />
		</javaClientGenerator>
		<!-- 要生成的那些表(更改tableName 和domainObjectName 就可以了) -->
		<table tableName="cluster_config" domainObjectName="ClusterConfig"
			enableCountByExample="false" enableUpdateByExample="false"
			enableDeleteByExample="false" enableSelectByExample="false"
			selectByExampleQueryId="false">

		</table>
	</context>
</generatorConfiguration>